{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "newLine": "lf",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "hgl.h",
    "downlevelIteration": true,
    "lib": [
      "webworker",
      "dom",
      "es2015",
      "es2017",
      "es2016",
      "es2018",
      "esnext"
    ],
    "noImplicitAny": false,
    "typeRoots": [
      "../node_modules/@types",
      "../typings"
    ],
    // "outDir": "lib",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "hypergl": [
        "../src"
      ],
    }
  },
  "include": [
    "load",
    "physics",
    "pointer",
    "sceneSetting",
    "stat",
    "../typings"
  ],
  "exclude": [
  ]
}