{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "newLine": "lf",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "hgl.h",
    "downlevelIteration": true,
    "lib": [
      "webworker",
      "dom",
      "es2015",
      "es2017",
      "es2016",
      "es2018",
      "esnext"
    ],
    "noImplicitAny": false,
    "typeRoots": [
      "./typings"
    ],
    "outDir": "build/output",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "gltf-loader-ts": [
        "./src/third_party/gltf-loader-ts/source"
      ]
    }
  },
  "include": [
    "load",
    "physics",
    "pointer",
    "sceneSetting",
    "stat"
  ],
  "exclude": [

  ]
}