{
    "psi-header.config": {
        "author": "dadigua",
        "authorEmail": "353272497@qq.com"
    },
    "psi-header.lang-config": [
        {
            "language": "javascript",
            "begin": "/*",
            "prefix": " * ",
            "end": " */",
            "blankLinesAfter": 2,
            "forceToTop": true
        },
    ],
    "psi-header.templates": [
        {
            "language": "javascript",
            "template": [
                "ProjectName: <<projectname>>",
                "FilePath: <<filerelativepath>>",
                "Created Date: <<dateformat('dddd, MMMM Do YYYY, h:mm:ss a')>>",
                "@author: <<author>>",
                "@summary: short description for the file",
                "-----",
                "Last Modified: <<dateformat('dddd, MMMM Do YYYY, h:mm:ss a')>>",
                "Modified By: ",
                "-----",
                "Copyright (c) <<year>> <<company>>"
            ]
        },
    ]
}